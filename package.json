{
  "name": "@vagnercardoso/skeleton-nodets",
  "version": "1.0.0",
  "description": "Skeleton express api + typescript",
  "keywords": [
    "nodejs",
    "express",
    "typescript"
  ],
  "main": "./dist/server.js",
  "homepage": "https://github.com/vagnercardosoweb/skeleton-nodets",
  "repository": {
    "type": "git",
    "url": "https://github.com/vagnercardosoweb/skeleton-nodets.git"
  },
  "bugs": {
    "email": "vagnercardosoweb@gmail.com",
    "url": "https://github.com/vagnercardosoweb/skeleton-nodets/issues"
  },
  "author": {
    "name": "Vagner Cardoso",
    "email": "vagnercardosoweb@gmail.com"
  },
  "maintainers": [
    {
      "name": "Vagner dos Santos Cardoso",
      "email": "vagnercardosoweb@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec sucrase-node ./src/server.ts --watch .",
    "dev:queues": "cross-env NODE_ENV=development nodemon --exec sucrase-node ./src/queues.ts --watch .",
    "dev:docker": "./scripts/dev-docker.sh",
    "prestart": "sequelize db:migrate",
    "start": "cross-env NODE_ENV=production pm2-runtime start ./ecosystem.config.js",
    "prebuild": "./scripts/pre-build.sh",
    "build": "sucrase ./src -d ./dist/src --transforms typescript,imports",
    "test": "cross-env NODE_ENV=test jest",
    "deploy:heroku": "yarn build && ./scripts/deploy-heroku.sh"
  },
  "dependencies": {
    "@sentry/node": "^5.11.1",
    "bcryptjs": "^2.4.3",
    "bull": "^3.12.1",
    "connect-redis": "^4.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.2",
    "nunjucks": "^3.2.0",
    "sequelize": "^5.21.3",
    "sharp": "^0.24.0",
    "socket.io": "^2.3.0",
    "youch": "^2.0.10",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.12.0",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "^0.0.45",
    "@types/ioredis": "^4.14.4",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongoose": "^5.5.43",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^13.5.0",
    "@types/nodemailer": "^6.4.0",
    "@types/nunjucks": "^3.1.3",
    "@types/sequelize": "^4.28.8",
    "@types/sharp": "^0.24.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@types/yup": "^0.26.28",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemailer-stub-transport": "^1.1.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.3",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
