{
  "name": "skeleton-nodets",
  "version": "1.0.0",
  "description": "Skeleton nodejs + typescript",
  "keywords": [
    "nodejs",
    "express",
    "typescript"
  ],
  "main": "./dist/server.js",
  "homepage": "https://github.com/vagnercardosoweb/skeleton-nodets",
  "repository": {
    "type": "git",
    "url": "https://github.com/vagnercardosoweb/skeleton-nodets.git"
  },
  "bugs": {
    "email": "vagnercardosoweb@gmail.com",
    "url": "https://github.com/vagnercardosoweb/skeleton-nodets/issues"
  },
  "author": {
    "name": "Vagner Cardoso",
    "email": "vagnercardosoweb@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "start": "yarn build && NODE_ENV=production pm2 startOrRestart ./ecosystem.config.js",
    "build": "tsc",
    "test": "NODE_ENV=testing jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@sentry/node": "^5.6.1",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^3.4.2",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.16.2",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "multer": "^1.4.2",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.3.0",
    "nunjucks": "^3.2.0",
    "sequelize": "^5.13.1",
    "sharp": "^0.23.0",
    "socket.io": "^2.2.0",
    "twig": "^1.13.3",
    "youch": "^2.0.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.11",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/helmet": "^0.0.44",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongoose": "^5.5.13",
    "@types/multer": "^1.3.9",
    "@types/node": "^12.7.2",
    "@types/nodemailer": "^6.2.1",
    "@types/nunjucks": "^3.1.2",
    "@types/sequelize": "^4.28.4",
    "@types/sharp": "^0.22.2",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.8",
    "@types/twig": "^1.12.2",
    "@types/yup": "^0.26.23",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "pm2": "^3.5.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
