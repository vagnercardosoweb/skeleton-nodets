{
  "name": "@vagnercardosoweb/nodets",
  "version": "1.0.0",
  "description": "Skeleton para desenvolvimento de pequenos e grandes projetos seja api ou normal.",
  "keywords": [
    "nodejs",
    "express",
    "typescript"
  ],
  "main": "./src/server.js",
  "author": {
    "name": "Vagner dos Santos Cardoso",
    "email": "vagnercardosoweb@gmail.com"
  },
  "maintainers": [
    {
      "name": "Vagner dos Santos Cardoso",
      "email": "vagnercardosoweb@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec sucrase-node ./src/server.ts --watch .",
    "dev:queues": "cross-env NODE_ENV=development nodemon --exec sucrase-node ./src/queues.ts --watch .",
    "dev:docker": "./scripts/dev-docker.sh",
    "prestart": "sequelize db:migrate",
    "start": "cross-env NODE_ENV=production pm2-runtime start ./ecosystem.config.js",
    "prebuild": "./scripts/pre-build.sh",
    "build": "sucrase ./src -d ./dist/src --transforms typescript,imports",
    "test": "cross-env NODE_ENV=test jest",
    "deploy:heroku": "yarn build && ./scripts/deploy-heroku.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{database,src,__tests__}/**/*.{js,ts}": [
      "eslint --ext js,ts --fix .",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@sentry/node": "^5.12.3",
    "bcryptjs": "^2.4.3",
    "bull": "^3.13.0",
    "connect-redis": "^4.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.2",
    "nunjucks": "^3.2.0",
    "sequelize": "^5.21.4",
    "sharp": "^0.24.0",
    "socket.io": "^2.3.0",
    "youch": "^2.0.10",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/mongodb": "^3.3.16",
    "@types/bull": "^3.12.0",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "^0.0.45",
    "@types/ioredis": "^4.14.7",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongoose": "^5.7.1",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.7.1",
    "@types/nodemailer": "^6.4.0",
    "@types/nunjucks": "^3.1.3",
    "@types/sequelize": "^4.28.8",
    "@types/sharp": "^0.24.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@types/yup": "^0.26.31",
    "@typescript-eslint/parser": "^2.19.2",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "nodemailer-stub-transport": "^1.1.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.3",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.12.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  }
}
